.Home {
  header {
    align center
    display: flex;
    background-color: red
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin: 2rem 0;
    img {
      max-width: 100%;
      height: auto;
      margin-top: 1rem;
    }
  }
  .card-container {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    margin-top: 2rem;
    .MuiCard-root {
      margin: 1rem;
      .MuiCardContent-root {
        text-align: center;
      }
    }
  }
}
